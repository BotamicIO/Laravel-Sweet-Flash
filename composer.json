{
    "name": "artisanry/sweet-alert",
    "description": "Alert messages through SweetAlert for Laravel 5.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-sweet-alert",
        "Laravel Sweet Alert"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "illuminate/session": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\SweetAlert\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\SweetAlert\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\SweetAlert\\SweetAlertServiceProvider"
            ],
            "aliases": {
                "SweetAlert": "Artisanry\\SweetAlert\\Facades\\SweetAlert"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
